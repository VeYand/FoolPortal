{
  "name": "fool_portal",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node eslint.mjs && tsc && vite build --emptyOutDir",
    "build-dev": "vite build --emptyOutDir",
    "generate-api": "docker run --rm -v ${PWD}/../:/var/www mirror.gcr.io/openapitools/openapi-generator-cli:v7.3.0 generate -i /var/www/backend/api/fool-portal-api.yaml -g typescript-fetch -o /var/www/frontend/src/app/api"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.8.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^15.12.0",
    "typescript": "~4.9.5",
    "vite": "^5.4.10",
    "vite-plugin-symfony": "^7.0.5"
  }
