<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="App\Subject\Domain\Model\Course" table="course">
        <id name="courseId" type="uuid_binary" column="course_id">
            <generator strategy="NONE"/>
        </id>

        <field name="teacherSubjectId" type="uuid_binary" column="teacher_subject_id"/>
        <field name="groupId" type="uuid_binary" column="group_id"/>
    </entity>
</doctrine-mapping>